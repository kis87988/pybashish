alias -- 'a'=demo-a
alias -- 'b'=demo-b
alias -- 'c'=demo-c
alias -- 'd'=demo-d
alias -- 'e'=demo-e
alias -- 'f'=demo-f
alias -- 'g'=demo-g
alias -- 'h'=demo-h
alias -- 'i'=demo-i
alias -- 'j'=demo-j
alias -- 'k'=demo-k
alias -- 'l'=demo-l
alias -- 'm'=demo-m
alias -- 'n'=demo-n
alias -- 'o'=demo-o
alias -- 'p'=demo-p
alias -- 'q'=demo-q
alias -- 'r'=demo-r
alias -- 's'=demo-s

function demo-a () { local x="export PS1='\\\$ '"; echo "+ $x" >&2; eval "$x"; }
function demo-b () { rm -fr ~/Desktop/pybashish; (set -xe; git clone https://github.com/pelavarre/pybashish.git); }
function demo-c () { local x="cd pybashish/"; echo "+ $x" >&2; $x; }
function demo-d () { (set -xe; cat README.md); }
function demo-e () { (set -xe; git branch -a); }
function demo-f () { (set -xe; git checkout pelavarre-patch-1); }
function demo-g () { (set -xe; less bin/hostname.py); }
function demo-h () { (set -xe; bin/argdoc.py --rip argdoc bin/hostname.py); }
function demo-i () { (set -xe; bin/argdoc.py --rip argparse bin/hostname.py); }
function demo-j () { local x="cd bin/"; echo "+ $x" >&2; $x; }
function demo-k () { (set -xe; argdoc.py --rip argdoc hostname.py >p.py); }
function demo-l () { (set -xe; chmod +x p.py); }
function demo-m () { (set -xe; vim p.py); }
function demo-n () { cp -p ../tests/demo-steps-p.py p.py; (set -xe; cat p.py); }
function demo-o () { (set -xe; p.py); }
function demo-p () { (set -xe; p.py 42); }
function demo-q () { (set -xe; diff -burp ../tests/demo-steps-p.py p.py); }
function demo-r () { (set -xe; rm -fr ~/Desktop/pybashish/); }
function demo-s () { local x="exit"; echo "+ logout" >&2; $x; }

pushd ~/Desktop >/dev/null
export PS1='(public) \$ '
